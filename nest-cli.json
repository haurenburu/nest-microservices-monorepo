{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/web/src",
  "monorepo": true,
  "root": "apps/web",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/web/tsconfig.app.json"
  },
  "projects": {
    "web": {
      "type": "application",
      "root": "apps/web",
      "entryFile": "main",
      "sourceRoot": "apps/web/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web/tsconfig.app.json"
      }
    },
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    }
  }
}